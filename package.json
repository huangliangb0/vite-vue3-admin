{
  "name": "vite-vue3-admin",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "json:server": "json-server --watch ./mock/db.json --port 3004",
    "serve": "vite",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "commit": "git-cz",
    "add:commit": "git add . && git-cz"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "ant-design-vue": "^3.2.10",
    "axios": "^0.27.2",
    "dayjs": "^1.11.3",
    "echarts": "^5.4.0",
    "element-resize-detector": "^1.2.4",
    "js-cookie": "^3.0.1",
    "json-server": "^0.17.0",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.16",
    "uuid": "^8.3.2",
    "vue": "^3.2.25",
    "vue-echarts": "^6.2.3",
    "vue-router": "^4.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@types/element-resize-detector": "^1.1.3",
    "@types/js-cookie": "^3.0.2",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.0.6",
    "@types/nprogress": "^0.2.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.30.7",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "1.3.10",
    "cz-customizable": "^6.9.1",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^8.3.0",
    "husky": "^8.0.1",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "rollup-plugin-visualizer": "^5.9.0",
    "typescript": "^4.5.4",
    "unplugin-vue-components": "^0.21.1",
    "unplugin-vue-define-options": "^0.6.2",
    "vite": "^2.9.9",
    "vite-plugin-eslint": "^1.7.0",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-ifdef": "^1.0.5",
    "vite-plugin-resolve-externals": "^0.2.2",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-eslint-parser": "^8.3.0",
    "vue-tsc": "^0.34.7"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
